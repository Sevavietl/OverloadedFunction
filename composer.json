{
    "name": "thorough-php/overloaded-function",
    "description": "Function overloading wrapper for php.",
    "version": "1.0",
    "type": "library",
    "keywords": [
        "php",
        "function",
        "overloading"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "sevavietl",
            "email": "sevavietl@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "thorough-php/type-guard": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "phpstan/phpstan": "^0.10.3",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "ThoroughPHP\\OverloadedFunction\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThoroughPHP\\OverloadedFunction\\Tests\\Unit\\": "tests/unit/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
